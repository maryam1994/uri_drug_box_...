<p id="notice"><%= notice %></p>

<h1>Listing Prows</h1>

<table>
  <thead>
    <tr>
      <th>Prescription</th>
      <th>Period</th>
      <th>qty</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prows.each do |prow| %>
      <tr>
        <td><%= prow.prescription_id %></td>
        <td><%= prow.period %></td>
        <td><%= prow.qty %></td>
        <td><%= link_to 'Show', prow %></td>
        <td><%= link_to 'Edit', edit_prow_path(prow) %></td>
        <td><%= link_to 'Delete', prow, method: :delete, data: { confirm: 'آیا اطمینان دارید؟' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Prow', new_prow_path%>

